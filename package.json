{
  "name": "ionic4-electron-starter",
  "version": "1.0.0",
  "author": "coturiv <coturiv@gmail.com>",
  "homepage": "https://github.com/coturiv/ionic4-electron-starter",
  "scripts": {
    "ng": "ng",
    "start": "npm run ele:build && concurrently --kill-others \"ng serve\" \"npm run ele:serve\"",
    "ele:lint": "tslint -p desktop/tsconfig.json",
    "ele:test": "node -v",
    "ele:build": "tsc -p desktop",
    "ele:start": "electron desktop",
    "ele:serve": "wait-on tcp:4200 && electron desktop --serve",
    "ele:copy-res": "cp -r resources/electron desktop/resources",
    "web:build": "ng build --prod --base-href=./",
    "web:copy": "cp -r www desktop",
    "build": "npm run web:build && npm run web:copy && npm run ele:build",
    "package": "electron-builder --config electron-builder.yml",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run ele:copy-res"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "@ionic/angular": "^4.7.1",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.1",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.803.0",
    "@angular-devkit/build-angular": "~0.803.0",
    "@angular-devkit/core": "~8.3.0",
    "@angular-devkit/schematics": "~8.3.0",
    "@angular/cli": "~8.3.0",
    "@angular/compiler": "~8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@ionic/angular-toolkit": "~2.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "concurrently": "^4.1.2",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "electron-is-dev": "^1.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.5",
    "wait-on": "^3.3.0"
  },
  "description": "A starter template built with electron, ionic, angular, typescript etc."
}
