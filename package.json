{
  "name": "ionic-electron-starter",
  "version": "1.0.0",
  "author": "coturiv <coturiv@gmail.com>",
  "homepage": "https://github.com/coturiv/ionic-electron-starter",
  "scripts": {
    "ng": "ng",
    "start": "npm run ele:build && concurrently --kill-others \"ng serve\" \"npm run ele:serve\"",
    "ele:lint": "tslint -p desktop/tsconfig.json",
    "ele:test": "node -v",
    "ele:build": "tsc -p desktop",
    "ele:start": "electron desktop",
    "ele:serve": "wait-on tcp:4200 && electron desktop --serve",
    "ele:copy-res": "cp -r resources/electron desktop/resources",
    "web:build": "ng build --prod --base-href=./",
    "web:copy": "cp -r www desktop",
    "build": "npm run web:build && npm run web:copy && npm run ele:build",
    "package": "electron-builder --config electron-builder.yml",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run ele:copy-res"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@ionic/angular": "^5.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@ionic/angular-toolkit": "^2.1.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^4.1.2",
    "electron": "^8.0.1",
    "electron-builder": "^22.3.2",
    "electron-is-dev": "^1.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "wait-on": "^4.0.0"
  },
  "description": "A starter template built with electron, ionic, angular, typescript etc."
}
